{
    "manifest_version": 3,
    "name": "Udemy Course Tracker",
    "description": "Track and manage your Udemy progress right from your browser.",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_title": "Udemy Course Tracker"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
      
    "permissions": ["storage"]
  }
  